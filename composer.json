{
    "name": "zegert/cakephp-two-factor-auth",
    "description": "CakePHP auth component and provider fot two-factor authentication",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "auth", "two-factor"],
    "homepage": "http://github.com/zegert/cakephp-two-factor-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Zegert Boele",
            "email": "contact@zegert.nl"
        }
    ],
    "require": {
        "cakephp/cakephp": "^4.0",
        "cakephp/authentication": "^2.0",
        "robthree/twofactorauth": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TwoFactorAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoFactorAuth\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
